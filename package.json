{
    "name": "alpine-day-picker",
    "version": "0.1.0",
    "description": "Alpine.js Day Picker component",
    "homepage": "https://github.com/rinodrummer/alpine-day-picker",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/rinodrummer/alpine-day-picker"
    },
    "bugs": {
        "url": "https://github.com/rinodrummer/alpine-day-picker/issues"
    },
    "type": "module",
    "files": [
        "dist",
        "lib",
        "src",
        "tsconfig.json",
        "tsconfig.build.json"
    ],
    "main": "./dist/main.umd.cjs",
    "module": "./dist/main.js",
    "types": "./dist/main.d.ts",
    "exports": {
        ".": {
            "import": "./dist/main.js",
            "require": "./dist/main.umd.cjs",
            "types": "./dist/main.d.ts"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc --p ./tsconfig.build.json && vite build",
        "preview": "vite preview",
        "test": "vitest",
        "lint": "eslint .",
        "typecheck": "tsc --p ./tsconfig.json --noEmit"
    },
    "dependencies": {
        "alpinejs": "^3.13.5"
    },
    "peerDependencies": {
        "alpinejs": "^3.13.5"
    },
    "devDependencies": {
        "@eslint/js": "^8.57.0",
        "@types/alpinejs": "^3.13.6",
        "@types/node": "^20.11.24",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "happy-dom": "^13.6.2",
        "prettier": "^3.2.5",
        "typescript": "^5.2.2",
        "typescript-eslint": "^7.1.1",
        "vite": "^5.1.4",
        "vite-plugin-dts": "^3.7.3",
        "vitest": "^1.3.1"
    },
    "author": {
        "name": "Gennaro Landolfi",
        "email": "code@rinodrummer.dev",
        "url": "https://rinodrummer.dev"
    }
}
